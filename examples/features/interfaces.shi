substract = (x, y) => x - y

a:real ~> { mod = (x) => a * x }
a:int ~> {
    mod = (x) => substract(a, x)
    sub = (x) => a - x
}

b = {
    a ~> {
        mod = (x) => a + x
        add = (x) => a + x
    }
    (x) => {
        if ("a".mod("b") == "ab") {
            x.mod(7).add(3).sub(1)
        } else {
            5
        }
    }
}

b(4).mod(1)
