divisibleRange = (n, from, to) => {
    if (from > to) {
        true 
    } else if (n % from != 0) {
        false
    } else {
        divisibleRange(n, from + 1, to)
    }
}

smallestDivisibleIn = (n, from, to) => {
    if (divisibleRange(n, from, to)) n else {
        smallestDivisibleIn(n + 1, from, to)
    }
}

smallestDivisibleIn(1, 1, 20)